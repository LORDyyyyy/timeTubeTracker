{
  "name": "timetubetracker",
  "version": "1.0.0",
  "description": "Calculates Youtube playlists total length",
  "main": "dist/server.js",
  "author": "Mohammed Khalid, Mohamed Tharwat",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --fix src",
    "format": "prettier --write src",
    "build": "tsc",
    "start": "node dist/server.js",
    "copy": "ncp src/views dist/views && ncp src/static dist/static",
    "copy:watch": "onchange 'src/**/*' -- npm run copy",
    "lint:watch": "onchange -k src -- npm run lint ",
    "format:watch": "onchange -k src -- npm run format ",
    "build:watch": "tsc -w",
    "start:watch": "nodemon dist/server.js",
    "format:lint": "onchange -k src -- npm-run-all lint format",
    "build:start": "npm-run-all -p build:watch start:watch",
    "all": "npm-run-all lint format build copy start",
    "all:watch": "onchange -k src -- npm-run-all lint format build copy start"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.3",
    "googleapis": "^134.0.0",
    "helmet": "^7.1.0",
    "hlputils": "^1.0.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.2",
    "ms": "^2.1.3",
    "redis": "^4.6.13",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/gapi": "^0.0.47",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mongoose": "^5.11.97",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.11.28",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "3.2.5",
    "typescript": "^4.9.5"
  }
}
