{
  "name": "timetubetracker",
  "version": "1.0.0",
  "description": "Calculates Youtube playlists total length",
  "main": "dist/server.js",
  "author": "Mohammed Khalid, Mohamed Tharwat",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:watch": "onchange src -- npm run build",
    "start": "node -r dotenv/config dist/server.js",
    "start:watch": "nodemon -r dotenv/config dist/server.js",
    "lint": "eslint --fix src",
    "lint:watch": "onchange -k src -- npm run lint ",
    "format": "prettier --write src",
    "format:watch": "onchange -k src -- npm run format ",
    "format:lint": "onchange -k src -- npm-run-all format lint",
    "build:start": "onchange -k src -- npm-run-all build start",
    "all": "onchange -k src -- npm-run-all format lint build start"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.2",
    "ms": "^2.1.3",
    "redis": "^4.6.13",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mongoose": "^5.11.97",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.11.28",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "3.2.5",
    "typescript": "^4.9.5"
  }
}
